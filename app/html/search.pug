include block_html/_style.pug
include block_html/_include.pug
.content.content--main
	+header()
	.cont
		.main-cont
			.content-wrap
				.content-nav
					.content-nav__wrap
						a(href="#").content-nav-el.content-nav-el--active Актиный пункт
						-
							var items = [
							'Устав партнёрства',
							'Доска почёта',
							'Состав партнёрства',
							'Комитеты партнёрства',
							'Контакты'
							];
						each item in items
							a(href="#").content-nav-el #{item}

					.content-nav__banner
						- for (var i=1;i<3;i++)
						.content-nav__banner-el.simle-banner Банер #{i}
				.content-info
					.main-title Результаты поиска
					- for (var i=0;i<12;i++)
						a(href="#").search-el
							.search-el__title Приветствуем партнеров конференции!....
							.search-el__text ВКомпания 1С-Рарус станет участником выставки Всероссийской независимой конференции по ITAM и SAM/1С-Рарус  — совместное предприятие фирм "1С" и "Рарус", создано в 1994 году. Сегодня 1С-Рарус — холдинг, филиалы которого работают в 17 городах России и за ее пределами.

		img(src="img/", alt="")
		.all-conf(style="background-image:url('img/item-7.jpg')")
			.filter.filter--red
			.all-conf__content
				.all-conf__title Все конференции itSMF России
				a(href="#").all-conf__get www.itsmfcon.ru
		.members
			.main-cont
				.members-wrap
					.members-list
						- for (var i=1;i<8;i++)
							.members-list__el
								img(src="img/sponsor-" + i + ".png", alt="")
					a(href="#").members-get Члены Форума

	+footer()
+modals()
include ./block_html/_script.pug
