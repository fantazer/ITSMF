	cp()
	.icon
		size 20px
		fill white
.header__menu-el
	+below(1024px)
		border-bottom 1px solid #ffffff0f
		padding: 0
	up()
	tr(all)
	position: relative
	padding: 5px 10px
	text-align: center
	width: 100%
	&:hover
		+below(1024px)
			background: none
		background: #a51e49
		.header__menu-sub
			height: auto
			visibility: visible
			opacity:1
			transform translateX(0px)
	&:after
		content ''
		position: absolute
		right: 0
		top 0px
		height: 100%
		width: 1px
		background: #ffffff45
	&:last-child
		&:after
			content ''
			display: none
